var map = ui.Map();

ui.root.clear();
ui.root.add(map);

// Visual features for map
map.setOptions('terrain');
map.style().set('cursor', 'crosshair');
map.setCenter(-87.6, 41.8, 6);

// Set markers for each lake
var locations = [[-91.764592, 47.931117],
  [-93.818537, 44.189743],
  [-93.41875, 43.902428]];

for (var l of locations) {
  new AdvancedMarkerElement({map: map, position: l});
}